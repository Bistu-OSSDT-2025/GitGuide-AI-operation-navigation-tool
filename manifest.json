{
  "manifest_version": 3,
  "name": "本地HTML跳转助手",
  "version": "1.0",
  "description": "根据用户指令跳转到HTML页面的指定板块或地标。",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "128": "icons/icon128.png"
    }
  },
  "host_permissions": ["file://*/*"]
}
